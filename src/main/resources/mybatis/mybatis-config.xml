<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!--<setting name="logImpl" value="STDOUT_LOGGING"/>-->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="logImpl" value="LOG4J2"/>
    </settings>

    <typeAliases>
        <!--给类型取简单的别名-->
        <typeAlias alias="Category" type="com.qf.pojo.Category"/>
        <typeAlias alias="Movie" type="com.qf.pojo.Movie"/>
        <typeAlias alias="User" type="com.qf.pojo.User"/>
        <typeAlias alias="Schedule" type="com.qf.pojo.Schedule"/>
        <typeAlias alias="MovieCategory" type="com.qf.pojo.MovieCategory"/>
    </typeAliases>

</configuration>